{
  "hosting": {
    "public": "out",
    "predeploy": [
      "npm run build"
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/store/**",
        "destination": "/store.html"
      },
      {
        "source": "/admin/store/**",
        "destination": "/admin/store.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
